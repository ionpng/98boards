{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*",
        "use": "@sveltejs/kit",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "api/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  